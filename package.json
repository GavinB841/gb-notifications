{
  "name": "gb-notifications",
  "version": "0.0.0-development",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test --watch=false",
    "test:lib-coverage": "ng test gb-notifications --code-coverage --watch=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "report-coverage:lib": "cat ./coverage/ng-alert/lcov.info | codecov",
    "build": "npm run build:lib && npm run build:demo",
    "build:lib": "npm run copy:readme && ng build ng-sortgrid",
    "build:showcase": "ng build gb-notifications-showcase --base-href='https://gavinb841.github.io/gb-notifications/'",
    "publish:demo": "npx angular-cli-ghpages --dir=./dist/gb-notifications-showcase",
    "copy:readme": "copyfiles ./README.md ./dist/ng-simple-alert",
    "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
    "semantic-release": "semantic-release",
    "adjust-version": "npm-run-all adjust-version:*",
    "adjust-version:lib": "rjp projects/gb-notifications/package.json version $VERSION",
    "adjust-version:main": "rjp package.json version $VERSION"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.0.0",
    "@angular/common": "~7.0.0",
    "@angular/compiler": "~7.0.0",
    "@angular/core": "~7.0.0",
    "@angular/forms": "~7.0.0",
    "@angular/http": "~7.0.0",
    "@angular/platform-browser": "~7.0.0",
    "@angular/platform-browser-dynamic": "~7.0.0",
    "@angular/router": "~7.0.0",
    "core-js": "^2.5.4",
    "node-sass": "^4.13.0",
    "rxjs": "~6.3.3",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.0",
    "@angular-devkit/build-ng-packagr": "~0.10.0",
    "@angular/cli": "~7.0.2",
    "@angular/compiler-cli": "~7.0.0",
    "@angular/language-service": "~7.0.0",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@commitlint/prompt": "^8.2.0",
    "@semantic-release/exec": "^3.3.8",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codecov": "^3.6.1",
    "codelyzer": "~4.5.0",
    "copyfiles": "^2.1.1",
    "husky": "^3.0.9",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lint-staged": "^9.4.3",
    "ng-packagr": "^4.2.0",
    "prettier": "^1.19.1",
    "protractor": "~5.4.0",
    "semantic-release": "^15.13.30",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "projects/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GavinB841/gb-notifications.git"
  }
}
